{
  "name": "stata-all-in-one",
  "displayName": "Stata All in One",
  "description": "Supercharge your Stata workflow in VS Code. An enhanced version of `Stata Outline` with advanced Syntax Highlighting + Code Hints + Smart Outline + Code Execution + Quick Editing. All your Stata needs in one place.",
  "version": "0.2.7",
  "publisher": "ZihaoVistonWang",
  "icon": "img/icon.png",
  "keywords": [
    "stata",
    "do",
    "ado",
    "outline",
    "statistics",
    "econometrics",
    "data analysis",
    "code runner",
    "syntax highlighting",
    "stata code",
    "stata do file",
    "stata outline",
    "stata runner"
  ],
  "engines": {
    "vscode": "^1.70.0"
  },
  "activationEvents": [
    "onLanguage:stata"
  ],
  "main": "./main.js",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "configuration": {
      "title": "Stata All in One",
      "properties": {
        "stata-all-in-one.numberingShow": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%config.numberingShow.description%"
        },
        "stata-all-in-one.numberingAdd": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%config.numberingAdd.description%"
        },
        "stata-all-in-one.showRunButton": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "%config.showRunButton.description%"
        },
        "stata-all-in-one.stataVersionOnMacOS": {
          "type": "string",
          "enum": [
            "",
            "StataMP",
            "StataIC",
            "StataSE"
          ],
          "default": "",
          "markdownDescription": "%config.stataVersionOnMacOS.description%"
        },
        "stata-all-in-one.stataPathOnWindows": {
          "type": "string",
          "default": "",
          "markdownDescription": "%config.stataPathOnWindows.description%"
        },
        "stata-all-in-one.stataStepDelayOnWindows": {
          "type": "number",
          "default": 100,
          "minimum": 50,
          "markdownDescription": "%config.stataStepDelayOnWindows.description%"
        },
        "stata-all-in-one.activateStataWindow": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "%config.activateStataWindow.description%"
        },
        "stata-all-in-one.commentStyle": {
          "type": "string",
          "enum": [
            "// ",
            "* ",
            "/* ... */"
          ],
          "default": "// ",
          "markdownDescription": "%config.commentStyle.description%"
        },
        "stata-all-in-one.separatorLength": {
          "type": "number",
          "default": 60,
          "minimum": 10,
          "markdownDescription": "%config.separatorLength.description%"
        },
        "stata-all-in-one.customCommands": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "reghdfe"
          ],
          "markdownDescription": "%config.customCommands.description%"
        },
        "stata-all-in-one.separatorSymmetric": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%config.separatorSymmetric.description%"
        }
      }
    },
    "languages": [
      {
        "id": "stata",
        "aliases": [
          "Stata",
          "stata"
        ],
        "extensions": [
          ".do",
          ".ado",
          ".mata"
        ],
        "configuration": "./language_config.json"
      }
    ],
    "grammars": [
      {
        "language": "stata",
        "scopeName": "source.stata",
        "path": "./grammars/stata.json"
      },
      {
        "scopeName": "stata.injection.custom-commands",
        "path": "./grammars/stata-custom.json",
        "injectTo": [
          "source.stata"
        ]
      }
    ],
    "commands": [
      {
        "command": "stata-all-in-one.setLevel1",
        "title": "Stata All in One: Set as Level 1 Heading"
      },
      {
        "command": "stata-all-in-one.setLevel2",
        "title": "Stata All in One: Set as Level 2 Heading"
      },
      {
        "command": "stata-all-in-one.setLevel3",
        "title": "Stata All in One: Set as Level 3 Heading"
      },
      {
        "command": "stata-all-in-one.setLevel4",
        "title": "Stata All in One: Set as Level 4 Heading"
      },
      {
        "command": "stata-all-in-one.setLevel5",
        "title": "Stata All in One: Set as Level 5 Heading"
      },
      {
        "command": "stata-all-in-one.setLevel6",
        "title": "Stata All in One: Set as Level 6 Heading"
      },
      {
        "command": "stata-all-in-one.clearHeading",
        "title": "Stata All in One: Clear Heading"
      },
      {
        "command": "stata-all-in-one.runSection",
        "title": "Stata All in One: Run Current Section",
        "icon": "$(play)"
      },
      {
        "command": "stata-all-in-one.reportBug",
        "title": "%command.reportBug.title%"
      },
      {
        "command": "stata-all-in-one.insertCustomSeparator",
        "title": "Stata All in One: Insert Custom Separator"
      },
      {
        "command": "stata-all-in-one.insertSeparatorDash",
        "title": "Stata All in One: Insert Dash Separator"
      },
      {
        "command": "stata-all-in-one.insertSeparatorEqual",
        "title": "Stata All in One: Insert Equal Separator"
      },
      {
        "command": "stata-all-in-one.insertSeparatorStar",
        "title": "Stata All in One: Insert Star Separator"
      },
      {
        "command": "stata-all-in-one.updateAllSeparators",
        "title": "Stata All in One: Update All Separators"
      },
      {
        "command": "stata-all-in-one.showHelp",
        "title": "Stata All in One: Show Help for Selected Command"
      },
      {
        "command": "stata-all-in-one.insertLineBreak",
        "title": "Stata All in One: Insert Stata Line Break"
      },
      {
        "command": "stata-all-in-one.resetStataVersionOnMacOS",
        "title": "Stata All in One: Reset Stata Version on macOS"
      },
      {
        "command": "stata-all-in-one.debugResetMigrationPrompt",
        "title": "Stata All in One: Debug Reset Migration Prompt"
      },
      {
        "command": "stata-all-in-one.debugTestUpdateNotification",
        "title": "Stata All in One: Debug Test Update Notification"
      }
    ],
    "keybindings": [
      {
        "command": "stata-all-in-one.runSection",
        "key": "ctrl+d",
        "mac": "cmd+d",
        "when": "editorTextFocus && editorLangId == stata"
      },
      {
        "command": "stata-all-in-one.insertSeparatorDash",
        "key": "ctrl+-",
        "mac": "cmd+-",
        "when": "editorTextFocus && editorLangId == stata"
      },
      {
        "command": "stata-all-in-one.insertSeparatorEqual",
        "key": "ctrl+=",
        "mac": "cmd+=",
        "when": "editorTextFocus && editorLangId == stata"
      },
      {
        "command": "stata-all-in-one.insertSeparatorStar",
        "key": "ctrl+shift+8",
        "mac": "cmd+shift+8",
        "when": "editorTextFocus && editorLangId == stata"
      },
      {
        "command": "stata-all-in-one.insertCustomSeparator",
        "key": "ctrl+alt+s",
        "mac": "ctrl+cmd+s",
        "when": "editorTextFocus && editorLangId == stata"
      },
      {
        "command": "stata-all-in-one.toggleComment",
        "key": "ctrl+/",
        "mac": "cmd+/",
        "when": "editorTextFocus && editorLangId == stata"
      },
      {
        "command": "stata-all-in-one.setLevel1",
        "key": "ctrl+1",
        "mac": "cmd+1",
        "when": "editorTextFocus && editorLangId == stata"
      },
      {
        "command": "stata-all-in-one.setLevel2",
        "key": "ctrl+2",
        "mac": "cmd+2",
        "when": "editorTextFocus && editorLangId == stata"
      },
      {
        "command": "stata-all-in-one.setLevel3",
        "key": "ctrl+3",
        "mac": "cmd+3",
        "when": "editorTextFocus && editorLangId == stata"
      },
      {
        "command": "stata-all-in-one.setLevel4",
        "key": "ctrl+4",
        "mac": "cmd+4",
        "when": "editorTextFocus && editorLangId == stata"
      },
      {
        "command": "stata-all-in-one.setLevel5",
        "key": "ctrl+5",
        "mac": "cmd+5",
        "when": "editorTextFocus && editorLangId == stata"
      },
      {
        "command": "stata-all-in-one.setLevel6",
        "key": "ctrl+6",
        "mac": "cmd+6",
        "when": "editorTextFocus && editorLangId == stata"
      },
      {
        "command": "stata-all-in-one.clearHeading",
        "key": "ctrl+0",
        "mac": "cmd+0",
        "when": "editorTextFocus && editorLangId == stata"
      },
      {
        "command": "stata-all-in-one.showHelp",
        "key": "ctrl+shift+h",
        "mac": "cmd+shift+h",
        "when": "editorTextFocus && editorLangId == stata"
      },
      {
        "command": "stata-all-in-one.insertLineBreak",
        "key": "shift+enter",
        "mac": "shift+enter",
        "when": "editorTextFocus && editorLangId == stata"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "stata-all-in-one.reportBug",
          "title": "%command.reportBug.short%",
          "when": "editorLangId == stata",
          "group": "navigation@0"
        },
        {
          "command": "stata-all-in-one.runSection",
          "when": "editorLangId == stata && config.stata-all-in-one.showRunButton",
          "group": "navigation@1"
        }
      ],
      "view/title": [
        {
          "command": "stata-all-in-one.runSection",
          "when": "view == outline && config.stata-all-in-one.showRunButton",
          "group": "navigation"
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ZihaoVistonWang/stata-all-in-one"
  }
}
